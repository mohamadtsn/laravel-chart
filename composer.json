{
    "name": "mohamadtsn/laravel-chart",
    "description": "The missing charting library on laravel, powered by Chartisan",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/mohamadtsn/laravel-chart.git",
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^7.14.1|^8.0|^9.0",
        "illuminate/http": "^7.14.1|^8.0|^9.0",
        "illuminate/console": "^7.14.1|^8.0|^9.0",
        "illuminate/support": "^7.14.1|^8.0|^9.0",
        "chartisan/php": "^1.2.0"
    },
    "authors": [
        {
            "name": "Mohammad",
            "email": "mohamad.borzoyi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ConsoleTVs\\Charts\\": "src/"
        },
        "files": [
            "tools/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConsoleTVs\\Charts\\ChartsServiceProvider"
            ]
        }
    }
}
